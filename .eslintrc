{
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "camelcase": 0,
    "curly": 0,
    "quotes": [ 2, "single" ],
    "strict": [ 2, "never" ],
    "no-process-exit": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": [ 2, { "vars": "all", "args": "none" } ],
  },
  "ecmaFeatures": {
    "modules": true
  }
}
